@using MVCBLOG.BLL.UIServices.Class
@using MVCBLOG.WEBUI.ViewModel
@model List<CounterViewModel>


@foreach (var item in Model)
{

    <a href="/Home/PostByCategoryUrl/@item.KategoriSeoLink"
       title="@item.CatDesc"
       class="menu-item list-group-item">
        @item.KategoriAdi
        <span class="badge" style="background-color: #397db3 !important;">@item.KategoridekiPostSayisi</span>
    </a>

}
